{
  "name": "kydriving-website",
  "private": true,
  "version": "1.0.0",
  "description": "광연자동차운전전문학원 프리미엄 웹사이트 - 2025년 리뉴얼",
  "type": "module",
  "author": "광연자동차운전전문학원",
  "license": "UNLICENSED",
  "keywords": [
    "driving-school",
    "education",
    "react",
    "typescript",
    "tailwindcss",
    "firebase",
    "vercel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/kydriving-website.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/kydriving-website/issues"
  },
  "homepage": "https://www.kydriving.co.kr",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "preview": "vite preview --host",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules package-lock.json",
    "deps:check": "npm audit && npm outdated",
    "deps:update": "npm update && npm audit fix",
    "test": "echo \"No tests specified\" && exit 0",
    "predeploy": "npm run lint && npm run type-check && npm run build",
    "deploy": "vercel --prod"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "firebase": "^12.7.0",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.563.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
